{
  "name": "alpaca-web",
  "version": "0.1.0",
  "private": false,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "copy-wasm": "sh scripts/copy_image_wasm.sh",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "test:unit": "vitest --run",
    "test:e2e": "start-server-and-test preview http://localhost:4173 'cypress run --e2e'",
    "test:e2e:dev": "start-server-and-test 'vite dev --port 4173' http://localhost:4173 'cypress open --e2e'",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-pattern .gitignore",
    "stylelint": "stylelint \"src/**/*.{vue,css,scss,less}\" --formatter=string --fix --ignore-pattern .gitignore",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "@primevue/themes": "^4.3.3",
    "@tabler/icons-vue": "^3.31.0",
    "@vueuse/core": "^13.1.0",
    "axios": "^1.7.9",
    "csv-parse": "^5.6.0",
    "grid-layout-plus": "^1.0.5",
    "lodash-es": "^4.17.21",
    "pinia": "^3.0.2",
    "primeicons": "^7.0.0",
    "primevue": "^4.0.4",
    "vue": "^3.4.29",
    "vue-3-slider-component": "^1.0.2",
    "vue-logger-plugin": "^2.2.3",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@pinia/testing": "^1.0.1",
    "@rushstack/eslint-patch": "^1.11.0",
    "@tsconfig/node20": "^20.1.4",
    "@types/jsdom": "^21.1.7",
    "@types/lodash-es": "^4.17.12",
    "@types/msgpack-lite": "^0.1.11",
    "@types/node": "^24.0.3",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.5.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "cypress": "^14.3.2",
    "eslint": "^9.25.1",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-cypress": "^4.3.0",
    "eslint-plugin-vue": "^10.0.0",
    "globals": "^16.0.0",
    "jsdom": "^24.1.0",
    "msgpack-lite": "^0.1.26",
    "npm-run-all2": "^7.0.2",
    "postcss-html": "^1.8.0",
    "prettier": "^3.5.3",
    "start-server-and-test": "^2.0.11",
    "stylelint-config-recommended-vue": "^1.6.0",
    "stylelint-config-standard-vue": "^1.0.0",
    "stylelint-declaration-strict-value": "^1.10.11",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.31.0",
    "vite": "^6.3.3",
    "vite-plugin-wasm-pack": "^0.1.12",
    "vitest": "^3.1.2",
    "vue-tsc": "^2.2.10"
  }
}
